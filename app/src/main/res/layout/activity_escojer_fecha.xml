<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EscojerFechaActivity">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/linearLayoutCompat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="152dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ESPECIALIDAD  DE MEDICINA GENERAL"
            tools:layout_editor_absoluteX="80dp"
            tools:layout_editor_absoluteY="71dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Por favor elija la fecha de su cita"
            tools:layout_editor_absoluteX="94dp"
            tools:layout_editor_absoluteY="124dp" />

        <Button
            android:id="@+id/button2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="@android:color/holo_blue_dark"
            android:text="DD/MM/AA"
            tools:layout_editor_absoluteX="156dp"
            tools:layout_editor_absoluteY="175dp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_height="41dp"
            android:text="Nombres y apellidos: Medico General1"
            tools:layout_editor_absoluteX="156dp"
            tools:layout_editor_absoluteY="259dp" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="match_parent"
            android:layout_height="186dp"
            app:srcCompat="@mipmap/ic_launcher"
            tools:layout_editor_absoluteX="122dp"
            tools:layout_editor_absoluteY="314dp"
            tools:srcCompat="@mipmap/ic_launcher_round" />

        <Button
            android:id="@+id/button3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="@android:color/holo_blue_dark"
            android:shadowColor="#2196F3"
            android:text="ver horas disponibles"
            tools:layout_editor_absoluteX="123dp"
            tools:layout_editor_absoluteY="514dp" />
    </androidx.appcompat.widget.LinearLayoutCompat>

</androidx.constraintlayout.widget.ConstraintLayout>